weight = input("What is your current weight?")
weight = int(weight)
height = input("what is your current height?")
height = int(height)

bmi = weight / (height**2)


if bmi < 18.5:
    print ("You are underweight")

elif (18.5< bmi < 25):
    print ("You have a normal weight")

elif (25< bmi < 30):
    print ("You are over weight")

elif (bmi > 30):
    print ("You are obese")
